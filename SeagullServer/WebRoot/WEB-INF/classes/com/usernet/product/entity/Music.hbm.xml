<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.usernet.product.entity.Music" table="music">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="time" type="java.lang.String">
			<column name="time" />
		</property>
		<property name="vol" type="java.lang.String">
			<column name="vol" />
		</property>
		<property name="coversrc" type="java.lang.String">
			<column name="coversrc" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="name" />
		</property>
		<property name="author" type="java.lang.String">
			<column name="author" />
		</property>
		<property name="musicsrc" type="java.lang.String">
			<column name="musicsrc" />
		</property>
		<property name="operator" type="java.lang.String">
			<column name="operator" />
		</property>
	</class>
</hibernate-mapping>
