package com.usernet.product.web.form;

import org.apache.struts.upload.FormFile;

import com.usernet.product.comm.BaseForm;
import com.usernet.product.entity.Product;
import com.usernet.product.entity.ProductPrice;

public class ProductForm extends BaseForm {

	private static final long serialVersionUID = -9138179683472319403L;

	private FormFile icon;

	private String pageNo;
	private String pageSize;

	private String name;
	private String manufacturer;
	private Integer status = -1;
	private Integer selectType;
	private Integer year;
	private Integer month;
	private Product product = new Product();
	private ProductPrice price = new ProductPrice();
	private Integer isRecommend = 2;

	private String softName; // 应用名称
	private String startTime;
	private String endTime;
	private Integer channelId;
	public Integer getChannelId() {
		return channelId;
	}

	public void setChannelId(Integer channelId) {
		this.channelId = channelId;
	}

	private int typeIds = -1; // 所属分类
	private int resIds = -1; // 所属主题

	public FormFile getIcon() {
		return icon;
	}

	public void setIcon(FormFile icon) {
		this.icon = icon;
	}

	public String getPageNo() {
		return pageNo;
	}

	public void setPageNo(String pageNo) {
		this.pageNo = pageNo;
	}

	public String getPageSize() {
		return pageSize;
	}

	public void setPageSize(String pageSize) {
		this.pageSize = pageSize;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Integer getYear() {
		return year;
	}

	public void setYear(Integer year) {
		this.year = year;
	}

	public Integer getMonth() {
		return month;
	}

	public void setMonth(Integer month) {
		this.month = month;
	}

	public String getManufacturer() {
		return manufacturer;
	}

	public void setManufacturer(String manufacturer) {
		this.manufacturer = manufacturer;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Integer getSelectType() {
		return selectType;
	}

	public void setSelectType(Integer selectType) {
		this.selectType = selectType;
	}

	public Product getProduct() {
		return product;
	}

	public void setProduct(Product product) {
		this.product = product;
	}

	public ProductPrice getPrice() {
		return price;
	}

	public void setPrice(ProductPrice price) {
		this.price = price;
	}

	public String getSoftName() {
		return softName;
	}

	public void setSoftName(String softName) {
		this.softName = softName;
	}

	public int getTypeIds() {
		return typeIds;
	}

	public void setTypeIds(int typeIds) {
		this.typeIds = typeIds;
	}

	public int getResIds() {
		return resIds;
	}

	public void setResIds(int resIds) {
		this.resIds = resIds;
	}

	public String getStartTime() {
		return startTime;
	}

	public void setStartTime(String startTime) {
		this.startTime = startTime;
	}

	public String getEndTime() {
		return endTime;
	}

	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}


	public Integer getIsRecommend() {
		return isRecommend;
	}

	public void setIsRecommend(Integer isRecommend) {
		this.isRecommend = isRecommend;
	}
}
