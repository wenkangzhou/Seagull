<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.usernet.product.entity.TaskCondition" table="task_condition">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="taskId" type="java.lang.Integer">
			<column name="taskId" />
		</property>
		<property name="type" type="java.lang.Integer">
			<column name="type" />
		</property>
		<property name="value" type="java.lang.Integer">
			<column name="value" />
		</property>
		<property name="sort" type="java.lang.Integer">
			<column name="sort" />
		</property>
		<property name="softId" type="java.lang.Integer">
			<column name="softId" />
		</property>
	</class>
</hibernate-mapping>
